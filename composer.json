{
    "name": "mclever/larasui-sdk",
    "description": "A Laravel SDK for interacting with the SUI blockchain.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mclever\\LarasuiSdk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mclever\\LarasuiSdk\\Providers\\SuiServiceProvider"
            ],
            "aliases": {
                "Sui": "Mclever\\LarasuiSdk\\Facades\\Sui"
            }
        }
    },
    "authors": [
        {
            "name": "Clever Emete",
            "email": "cedextechs@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    }
}
